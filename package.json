{
  "name": "remix-example-turborepo-vercel",
  "private": true,
  "packageManager": "pnpm@7.8.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "clean": "git clean -fdx -e .vercel .",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write .",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck"
  },
  "dependencies": {
    "prettier": "2.7.1",
    "turbo": "1.4.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
