{
  "name": "remix-example-turborepo-vercel",
  "private": true,
  "packageManager": "pnpm@7.0.0-rc.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "clean": "git clean -fdx -e .vercel .",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write .",
    "lint": "turbo run lint"
  },
  "dependencies": {
    "prettier": "^2.6.2",
    "turbo": "^1.2.5"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
